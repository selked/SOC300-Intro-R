example <- our_gss
breaks <- pretty(range(example$realrinc, na.rm = TRUE),
n = nclass.Sturges(example$realrinc),
min.n = 1)
ggplot(our_gss, aes(realrinc)) +
geom_histogram(breaks = breaks) +
geom_vline(aes(xintercept = mean(realrinc, na.rm = TRUE), color = "mean")) +
